{"_id":"DHIVx86ygi7rry3S","key":"pbta.sheetConfig","value":"{\"tomlString\":\"# Configure Rolls\\nrollFormula = \\\"2d6\\\"\\n\\n[statToggle]\\n  label = \\\"Mal soigné\\\"\\n  modifier = \\\"-1\\\"\\n\\n# Define roll result ranges.\\n[rollResults]\\n  [rollResults.failure]\\n    range = \\\"6-\\\"\\n    label = \\\"Complications...\\\"\\n  [rollResults.partial]\\n    range = \\\"7-9\\\"\\n    label = \\\"Succès partiel\\\"\\n  [rollResults.success]\\n    range = \\\"10-12\\\"\\n    label = \\\"Succès !\\\"\\n\\n########################################\\n## CHARACTERS ##########################\\n########################################\\n# Define the character group.\\n[character]\\n\\n  # Define stats.\\n  [character.stats]\\n    cran = \\\"Cran\\\"\\n    pro = \\\"Pro\\\"\\n    chair = \\\"Chair\\\"\\n    esprit = \\\"Esprit\\\"\\n    style = \\\"Style\\\"\\n    synth = \\\"Synth\\\"\\n\\n  # Define attributes.\\n  [character.attributesTop]\\n    [character.attributesTop.harm]\\n      type = \\\"Clock\\\"\\n      label = \\\"Blessure\\\"\\n      max = 6\\n    [character.attributesTop.armor]\\n      type = \\\"Number\\\"\\n      label = \\\"Armure\\\"\\n    [character.attributesTop.hold]\\n      type = \\\"Resource\\\"\\n      label = \\\"Retenues\\\"\\n    [character.attributesTop.xp]\\n      type = \\\"Xp\\\"\\n      label = \\\"Xp\\\"\\n      max = 5\\n\\n  # Define sidebar details.\\n  [character.attributesLeft]\\n    [character.attributesLeft.infos]\\n      type = \\\"Number\\\"\\n      label = \\\"Infos\\\"\\n    [character.attributesLeft.matos]\\n      type = \\\"Number\\\"\\n      label = \\\"Matos\\\"\\n    [character.attributesLeft.cred]\\n      type = \\\"Number\\\"\\n      label = \\\"Cred\\\"\\n    [character.attributesLeft.harmConditions]\\n      type = \\\"ListMany\\\"\\n      label = \\\"Conditions\\\"\\n      options = [\\n        \\\"Inconscient\\\",\\n        \\\"Aveuglé\\\",\\n        \\\"Assourdi\\\",\\n        \\\"Paralysé\\\",\\n        \\\"Etourdi\\\",\\n        \\\"Affaibli\\\",\\n        \\\"Mourrant\\\",\\n      ]\\n    [character.attributesLeft.look]\\n      type = \\\"LongText\\\"\\n      label = \\\"Apparence\\\"\\n\\n  # Define groups for moves.\\n  [character.moveTypes]\\n    directives = \\\"Directives\\\"\\n    basic = \\\"Actions de base\\\"\\n    peripheral = \\\"Actions spéciales\\\"\\n    class = \\\"Actions de classe\\\"\\n    matrix = \\\"Actions de matrice\\\"\\n\\n  # Define groups for equipment.\\n  [character.equipmentTypes]\\n    cyber = \\\"Cyber\\\"\\n    arme = \\\"Armes\\\"\\n    equipement = \\\"Equipements\\\"\\n    contacts = \\\"Contacts\\\"\\n    liens = \\\"Liens\\\"\\n    programmes = \\\"Programmes\\\"\\n\\n########################################\\n## NPCS ################################\\n########################################\\n# Define stats.\\n[npc]\\n  # Define attributes.\\n  [npc.attributesTop]\\n    [npc.attributesTop.harm]\\n      type = \\\"Resource\\\"\\n      label = \\\"Blessure\\\"\\n    [npc.attributesTop.damage]\\n      type = \\\"Roll\\\"\\n      label = \\\"Damage\\\"\\n      default = \\\"d6\\\"\\n    [npc.attributesTop.loot]\\n      type = \\\"Text\\\"\\n      label = \\\"Loot\\\"\\n  [npc.attributesLeft]\\n    [npc.attributesLeft.look]\\n      type = \\\"LongText\\\"\\n      label = \\\"Apparence\\\"\\n    [npc.attributesLeft.drive]\\n      type = \\\"LongText\\\"\\n      label = \\\"Drive\\\"\\n\\n  # Define logical groups for moves.\\n  [npc.moveTypes]\\n    gm = \\\"GM Moves\\\"\",\"computed\":{\"rollFormula\":\"2d6\",\"statToggle\":{\"label\":\"Mal soigné\",\"modifier\":\"-1\"},\"rollResults\":{\"failure\":{\"range\":\"6-\",\"label\":\"Complications...\"},\"partial\":{\"range\":\"7-9\",\"label\":\"Succès partiel\"},\"success\":{\"range\":\"10-12\",\"label\":\"Succès !\"}},\"character\":{\"stats\":{\"cran\":\"Cran\",\"pro\":\"Pro\",\"chair\":\"Chair\",\"esprit\":\"Esprit\",\"style\":\"Style\",\"synth\":\"Synth\"},\"attributesTop\":{\"harm\":{\"type\":\"Clock\",\"label\":\"Blessure\",\"max\":6},\"armor\":{\"type\":\"Number\",\"label\":\"Armure\"},\"hold\":{\"type\":\"Resource\",\"label\":\"Retenues\"},\"xp\":{\"type\":\"Xp\",\"label\":\"Xp\",\"max\":5}},\"attributesLeft\":{\"infos\":{\"type\":\"Number\",\"label\":\"Infos\"},\"matos\":{\"type\":\"Number\",\"label\":\"Matos\"},\"cred\":{\"type\":\"Number\",\"label\":\"Cred\"},\"harmConditions\":{\"type\":\"ListMany\",\"label\":\"Conditions\",\"options\":[\"Inconscient\",\"Aveuglé\",\"Assourdi\",\"Paralysé\",\"Etourdi\",\"Affaibli\",\"Mourrant\"]},\"look\":{\"type\":\"LongText\",\"label\":\"Apparence\"}},\"moveTypes\":{\"directives\":\"Directives\",\"basic\":\"Actions de base\",\"peripheral\":\"Actions spéciales\",\"class\":\"Actions de classe\",\"matrix\":\"Actions de matrice\"},\"equipmentTypes\":{\"cyber\":\"Cyber\",\"arme\":\"Armes\",\"equipement\":\"Equipements\",\"contacts\":\"Contacts\",\"liens\":\"Liens\",\"programmes\":\"Programmes\"}},\"npc\":{\"attributesTop\":{\"harm\":{\"type\":\"Resource\",\"label\":\"Blessure\"},\"damage\":{\"type\":\"Roll\",\"label\":\"Damage\",\"default\":\"d6\"},\"loot\":{\"type\":\"Text\",\"label\":\"Loot\"}},\"attributesLeft\":{\"look\":{\"type\":\"LongText\",\"label\":\"Apparence\"},\"drive\":{\"type\":\"LongText\",\"label\":\"Drive\"}},\"moveTypes\":{\"gm\":\"GM Moves\"}}}}"}
